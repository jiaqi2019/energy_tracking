{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "enablePullDownRefresh": false,
        "onReachBottomDistance": 50,
        "enableScroll": true
      }
    },
    {
      "path": "pages/statistic/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/mood-form/index",
      "style": {
        "navigationBarTitleText": "当下你感受如何"
      }
    },
    {
      "path": "pages/settings/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "uni_modules/uni-id-pages/pages/register/register",
      "style": {
        "navigationBarTitleText": "注册"
      }
    },
    {
      "path": "uni_modules/uni-id-pages/pages/login/login-withpwd",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "uni_modules/uni-id-pages/pages/retrieve/retrieve",
      "style": {
        "navigationBarTitleText": "找回密码"
      }
    },
    {
      "path": "pages/analyze/index",
      "style": {
        "navigationStyle": "custom",
        "enableScroll": true,
        "disableScroll": false
      }
    }
  ],
  "tabBar": {
    "borderStyle": "white",
    "height": "50px",
    "blurEffect": "extralight",
    /*  #ifdef   MP-WEIXIN  */
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/tabbar/todolist.png",
        "selectedIconPath": "static/tabbar/todolist-active.png"
      },
      {
        "pagePath": "pages/analyze/index",
        "iconPath": "static/tabbar/tubiao.png",
        "selectedIconPath": "static/tabbar/tubiao-active.png"
      },
      {
        "pagePath": "pages/settings/index",
        "iconPath": "static/tabbar/settings.png",
        "selectedIconPath": "static/tabbar/settings-active.png"
      }
    ],
    /*  #endif  */
    /*  #ifdef APP-PLUS  */
    "iconfontSrc": "static/icon/iconfont.ttf",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconfont": {
          "text": "\ue60f",
          "selectedText": "\ue60f",
          "fontSize": "30px",
          "selectedColor": "#7bcdbf"
        }
      },
      {
        "pagePath": "pages/analyze/index",
        "iconfont": {
          "text": "\ue6d8",
          "selectedText": "\ue6d8",
          "selectedColor": "#7bcdbf",
          "fontSize": "30px"
        }
      },
      {
        "pagePath": "pages/settings/index",
        "iconfont": {
          "text": "\ue851",
          "selectedText": "\ue851",
          "selectedColor": "#7bcdbf",
          "fontSize": "30px"
        }
      }
    ]
    /*  #endif  */
  },
  "globalStyle": {}
}
