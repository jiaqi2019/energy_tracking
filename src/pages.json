{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/statistic/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path" : "pages/mood-form/index"
    },
    {
      "path": "uni_modules/uni-id-pages/pages/register/register",
      "style": {
        "navigationBarTitleText": "注册"
      }
    },
    {
      "path": "uni_modules/uni-id-pages/pages/login/login-withpwd",
      "style": {
        "navigationBarTitleText": "登录"
      }
    }
  ],
  "tabBar": {
   "color": "#161823",
   "backgroundColor": "#16182314",
   "selectedColor": "",
   "borderStyle":"white",
   "height": "50px",
   "fontSize": "12px",
   "blurEffect":"extralight",
   "iconfontSrc":"static/icon/iconfont.ttf",
   "iconWidth": "20px",
   "list": [
     {
       "pagePath": "pages/index/index",
       "text": "觉察",
       "iconfont": {
         "text": "\ue603",
         "fontSize": "18px",
         "color": "#ff0000"
       }
     },
     {
       "pagePath": "pages/statistic/index",
       "text": "解析",
       "iconfont": {
         "text": "\ue603",
         "fontSize": "18px",
         "color": "#ff0000"
       }
     }
   ]
  },
  "globalStyle": {
  }
}
